
[Unit]
Description=Telegram Bot
After=network.target

[Service]
User=test_user
Group=test_user
WorkingDirectory=/home/test_user/studing-telegram/
Environment="PYTHONPATH=/home/test_user/studing-telegram/"
ExecStart=/home/test_user/studing-telegram/.venv/bin/python /home/test_user/studing-telegram/emodji.py

[Install]
WantedBy=multi-user.target